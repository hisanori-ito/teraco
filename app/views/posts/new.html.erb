<h1>Posts#new</h1>
<%= form_with model:@post do |f| %>
  <h4>title</h4>
    <%= f.text_field :title %>
  <h4>thumbnail</h4>
    <%= f.file_field :thumbnail %>
  <h4>video</h4>
    <%= f.file_field :video %>
  <h4>content</h4>
    <%= f.text_area :content %>
  <h4>tags</h4>
    <%= f.text_field :tag %>
  <%= f.submit "投稿" %>
<% end %>